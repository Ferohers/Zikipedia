{
  "name": "Zikipedia",
  "description": "redirects wikipedia to 0wikipedia",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "permissions": [
    "webRequest",
    "*://*.wikipedia.org/*",
    "*://*.www.wikipedia.org/*",
    "webRequestBlocking"
  ]
}
